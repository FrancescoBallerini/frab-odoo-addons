<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template
        id="address_checkout_location_autocomplete"
        inherit_id="website_sale.address"
        name="Website Address Checkout Location Autocomplete"
    >

        <!-- Add selector to load widget WebsiteSaleZipcodeAutocomplete -->
        <xpath expr="//form" position="attributes">
            <attribute
                name="class"
                add="website_sale_zipcode_autocomplete"
                separator=" "
            />
        </xpath>

    </template>
</odoo>
